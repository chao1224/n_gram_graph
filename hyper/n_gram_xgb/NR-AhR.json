{
  "max_depth": 10,
  "learning_rate": 0.03,
  "n_estimators": 300,
  "booster": "gbtree",
  "objective": "binary:logistic",
  "subsample": 1,
  "colsample_bylevel": 0.5,
  "colsample_bytree": 1,
  "min_child_weight": 1,
  "reg_alpha": 0.3,
  "reg_lambda": 0.7,
  "scale_pos_weight": 1,

  "early_stopping": {
    "round": 100,
    "eval_metric": "AUC[ROC]"
  },

  "enrichment_factor":{
    "ratio_list": [0.02, 0.01, 0.0015, 0.001]
  },
  "random_seed": 1337,
  "max_delta_step": 0,
  "label_name_list": ["Keck_Pria_AS_Retest"]
}